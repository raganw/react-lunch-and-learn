{
  "name": "react-lunch-and-learn",
  "version": "0.0.1",
  "description": "React lunch and learn",
  "repository": "https://bitbucket.org/raganw/react-lunch-and-learn",
  "config": {
    "buildDir": "./build",
    "buildDirTests": "./build_tests",
    "devHost": "localhost",
    "devPort": 8003
  },
  "scripts": {
    "build": "NODE_ENV=production npm run webpack",
    "check": "NODE_ENV=test karma start --single-run && npm run start",
    "clean": "rimraf $npm_package_config_buildDir && mkdir $npm_package_config_buildDir",
    "env": "env",
    "lint": "eslint --ext .js --ext .jsx ./app ./webpack && echo No linting errors.",
    "prebuild": "npm run clean",
    "precheck": "npm install && npm run lint",
    "-prestart": "npm install",
    "-pretest": "npm install && npm run lint",
    "pretest-travis": "npm install && npm run lint",
    "start": "NODE_ENV=development node dev-server ./webpack/config",
    "test": "NODE_ENV=test karma start",
    "test-travis": "NODE_ENV=test karma start --single-run",
    "webpack": "webpack --colors --progress --config ./webpack/config"
  },
  "dependencies": {
    "classnames": "^2.2.0",
    "firebase": "^2.3.1",
    "lodash": "^3.10.1",
    "normalize.css": "^3.0.3",
    "react": "^0.14.2",
    "react-dom": "^0.14.2"
  },
  "devDependencies": {
    "autoprefixer": "^6.1.0",
    "babel-core": "^6.1.2",
    "babel-eslint": "^4.1.5",
    "babel-loader": "^6.1.0",
    "babel-polyfill": "^6.0.16",
    "babel-preset-es2015": "^6.1.2",
    "babel-preset-react": "^6.1.2",
    "css-loader": "^0.22.0",
    "ejs": "^2.3.4",
    "eslint": "^1.9.0",
    "eslint-config-airbnb": "^1.0.0",
    "eslint-loader": "^1.1.1",
    "eslint-plugin-react": "^3.8.0",
    "extract-text-webpack-plugin": "^0.9.1",
    "file-loader": "^0.8.4",
    "front-matter": "^2.0.0",
    "glob": "^5.0.15",
    "html-loader": "^0.3.0",
    "json-loader": "^0.5.3",
    "karma": "^0.13.15",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^0.2.1",
    "karma-mocha": "^0.2.0",
    "karma-mocha-reporter": "^1.1.1",
    "karma-phantomjs-launcher": "^0.2.1",
    "karma-sinon": "^1.0.4",
    "karma-sinon-chai": "^1.1.0",
    "karma-source-map-support": "^1.1.0",
    "karma-sourcemap-loader": "^0.3.6",
    "karma-webpack": "^1.7.0",
    "markdown": "git://github.com/cadorn/markdown-js.git",
    "mocha": "^2.3.3",
    "node-sass": "^3.4.1",
    "opn": "^3.0.3",
    "phantomjs": "^1.9.18",
    "postcss-loader": "^0.7.0",
    "raw-loader": "^0.5.1",
    "react-addons-test-utils": "^0.14.2",
    "react-hot-loader": "^1.3.0",
    "rimraf": "^2.4.3",
    "sass-loader": "^3.1.1",
    "sinon": "^1.17.2",
    "style-loader": "^0.13.0",
    "template-html-loader": "0.0.3",
    "url-loader": "^0.5.6",
    "webpack": "^1.12.4",
    "webpack-dev-server": "^1.12.1"
  },
  "engines": {
    "node": ">=0.12.0"
  }
}
